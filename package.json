{
  "name": "youtube-viewbot",
  "version": "1.0.2",
  "description": "This is a youtube viewbot built on electron",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . Youtube-Viewbot --overwrite --asar --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.ProductName=\"Youtube-Viewbot\"",
    "package-linux": "electron-packager . Youtube-Viewbot --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "author": "Audition-CSBlock",
  "license": "MIT",
  "dependencies": {
    "cryptiles": "^4.1.3",
    "deep-extend": "^0.6.0",
    "electron-log": "^4.2.2",
    "electron-packager": "^12.0.0"
  },
  "devDependencies": {
    "electron": "^2.0.14",
    "hoek": "^4.2.1"
  }
}
